
@model BitirmeProjesi.Models.Turlar
@{
    ViewData["Title"] = "YeniTur";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Yeni Tur Ekleme</h1>
<form class="form-group" method="post">

    @Html.LabelFor(x=>x.UlkeIsim)
    @Html.TextBoxFor(x=>x.UlkeIsim,new{@class="form-control"})
    </br>
    @Html.LabelFor(x=>x.SehirIsim)
    @Html.TextBoxFor(x=>x.SehirIsim,new{@class="form-control"})
    </br>
    @Html.LabelFor(x=>x.UlkeResimUrl)
    @Html.TextBoxFor(x=>x.UlkeResimUrl,new{@class="form-control"})
    </br>
    @Html.LabelFor(x=>x.TurKacGun)
    @Html.TextBoxFor(x=>x.TurKacGun,new{@class="form-control"})
    </br>
    @Html.LabelFor(x=>x.TurFiyat)
    @Html.TextBoxFor(x=>x.TurFiyat,new{@class="form-control"})
    </br>
    <button class="btn btn-primary">Yeni Tur Ekle</button>
</form>

